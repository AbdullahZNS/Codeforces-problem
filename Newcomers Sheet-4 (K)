#include<stdio.h>
int main()
{
 char S[50];
 char T[50];

 int N,i;
 int cnt_S, cnt_T = 0;
 scanf("%d",&N);

 scanf("%s",S);
 scanf(" %s",T);

 for(i = 0; S[i] != '\0'; i++)
 {
  cnt_S++;
 }

  for(i = 0; T[i] != '\0'; i++)
 {
  cnt_T++;
 }

 if(cnt_S == cnt_T)
 {
  for(i = 0; i < cnt_S; i++)
  {
  printf("%s%s\n",S[i],T[i]);
  }
 }


    return 0;
}
